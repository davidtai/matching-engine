{
  "presets": [
    [
      "@babel/preset-env", {
        "modules": false
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {
        "legacy": false,
        "decoratorsBeforeExport": true
    }],
    ["@babel/plugin-proposal-class-properties", { "loose": false }],
    ["@babel/plugin-proposal-object-rest-spread"]
  ],
  "exclude": [],
  "env": {
    "test": {
      "presets": [["@babel/preset-env"]]
    }
  }
}
